
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/fe2c0000.mmc/mmc_host/mmc1/mmc1:0001/mmc1:0001:1'
	option channel '1'
	option band '2g'
	option htmode 'HT40'
	option disabled '0'
	option cell_density '0'
	option country 'CN'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'H-WiFi'
	option encryption 'psk2'
	option key '23456701'
	option macaddr '56:C0:AA:16:D7:62'

