
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering '1'

config device
	option name 'br-lan'
	option type 'bridge'
	option bridge_empty '1'
	option macaddr '56:C0:AA:16:D7:62'

config device
	option name 'eth0'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option ipaddr '192.168.7.254'
	option netmask '255.255.255.0'
	option ip6ifaceid '::254'
	list dns '192.168.7.254'
	option ip6assign '64'

config interface 'docker'
	option device 'docker0'
	option proto 'none'

config device
	option type 'bridge'
	option name 'docker0'

config interface 'zerotier'
	option proto 'dhcp'
	option device 'ztks5zxcy4'

config interface 'wan'
	option proto 'pppoe'
	option device 'eth0'
	option username 'hfi45593'
	option password '958199'
	option ipv6 'auto'

config interface 'wan6'
	option proto 'dhcpv6'
	option device 'eth0'
	option reqaddress 'try'
	option reqprefix 'auto'
	option norelease '1'
	list ip6class 'local'
	list ip6class 'wan_6'

